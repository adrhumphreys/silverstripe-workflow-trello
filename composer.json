{
    "name": "silverstripe/workflow-trello",
    "description": "Integrate with Trello in the workflow module.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Trello"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4",
        "silverstripe/admin": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Workflow\\Trello\\": "src/",
            "SilverStripe\\Workflow\\Trello\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
